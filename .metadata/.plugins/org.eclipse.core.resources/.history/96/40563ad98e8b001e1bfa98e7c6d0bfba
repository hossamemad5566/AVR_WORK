#define SET_BIT(REG,PIN) (REG|=(1<<PIN))
#define CLEAR_BIT(REG,PIN) (REG&=(~(1<<PIN)))
#define TOGGLE_BIT(REG,PIN) (REG^=(1<<PIN))
/* ********************** Macro Section End   ************************* */

